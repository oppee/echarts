/*我的账户search*/
.weui_search_bar{padding: 8px 15px;}
.btn_group.weui_panel:after, .btn_group.weui_panel:before, .btn_group .weui_panel_hd:after{border: none;}
.btn_group{margin: 0;}
.btn_group .weui_panel_bd {padding: 10px 15px;}
.btn_group .weui_panel_bd a{font-size: 12px;border-radius: 20px;height: 32px;line-height: 32px;}
.black_bor {border-color: #d2d2d2;color: #666;}
.machine a{display: inline-block;padding: 0 15px;margin: 12px 8px 0 !important;float: left;}
.btn_group .machine{padding-left: 8px;padding-right: 8px;}
.black_bg {background: #e6e6e6;}
.blue_bor {border-color: #00a0e9;color: #00a0e9;background: #fff;}
.btn_items .weui_panel_hd{padding: 3px 15px 3px;}
/*.btn_group{margin-bottom: 200px !important;}*/
.btn_group .btn_sm .weui_panel_hd{padding-bottom: 0;}
.btn_group .btn_sm .machine{padding-top: 0;}
.botton_btn{position: fixed;width: 100%;bottom: 0;left: 0;}
.botton_btn .weui-col-50 .reset_btn{color: #00a0e9;background: #e8f3f9;}
.botton_btn .weui_btn:after{border: none;border-radius: 0;}
.botton_btn .weui-col-50 .success_btn {color: #fff;background: #218fc8;}
.botton_btn .weui-col-50 a{border-radius: 0;font-size: 15px;height: 45px;line-height: 45px;}
/*.weui_search_outer {height: 32px;line-height: 32px;}*/
.weui_icon_search:before{font-size: 17px;}
.weui_search_inner .weui_icon_search{top: 11px;line-height: 25px;left: inherit;right: 15px !important;}
.weui_search_cancel{line-height: 34px;}
.search_focus .weui_icon_clear {background: none;border-radius: 0;top: 1px;right: 14px;color: #333;}
.search_focus .weui_icon_clear:before{content: "\EA0F";}
.search_focus .weui_search_input:not(:valid)~.weui_icon_clear{display: none;}
.weui_tab .weui_cell_title{border-bottom: 1px solid #e6e6e6; }
.weui_cells_form .weui_msg_desc, #login_form .weui_msg_desc {font-size: 14px;color: #999;max-height: 217px;overflow-y: auto;text-indent: 20px;}
.btn_sm .weui_panel_bd input{width: 36%;height: 32px;display: inline-block;border-radius: 20px;border: 1px solid #dadada;margin: 15px 8px 0 !important;text-align: center;line-height: 32px;z-index: 0;}
.weui_tab{height: auto;}
#main{width: 98%;height:280px;margin:10px;}
.weui_tab_bd_item{overflow: inherit;}
#search_input:focus, #search_input:active{border: none;}
.mychart_box {min-width: 305px;max-width: 625px;position: relative;overflow-x: auto;margin: 0 0 20px;height: 224px;overflow-y: hidden;}
.mychart_box .center-align { text-align: center; padding: 10px 0 2px;}
.mychart{position: absolute;width: 600px;left: -15px; }
#tab2 .mychart {top:-28px; }
#tab3 .mychart {top:-18px; }

.title_box {width: 12%;float: left;text-align: center;font-size: 13px;color: #666;}
.title_items, .tips_items{width: 44%;float: left;text-align: center;}
.title_items span, .tips_items span{font-size: 13px;color: #666;}


.mychart_content b { font-weight: normal;}
.mychart_content b.blue{color: #00a0e9;padding: 0 1px;font-weight: normal;}
.mychart_content b.green{color: #0dc91a;padding: 0 1px;font-weight: normal;}
.mychart_content b.gay{color:#006a65; padding: 0 1px;font-weight: normal;}
.mychart_content .weui-row {  border-bottom: 1px solid #dadada; padding: 5px 0; }
.mychart_content .center-align { padding: 5px 0; text-align: center; font-size: 14px; color: #999;}
.title_items span{position: relative;padding: 0 2px;}
.title_items span:after{width: 1px;border-right: 1px solid #dadada;height: 14px;position: absolute;content: '';top: 3px;}
.title_items span.bor_non:after{content: '';width: 0;border: none;}
.tips_items span b.blue{color: #00a0e9;padding: 0 1px;font-weight: normal;}
.tips_items span b.green{color: #0dc91a;padding: 0 1px;font-weight: normal;}
.tips_items span b.gay{color:#006a65; padding: 0 1px;font-weight: normal;}
.weui_msg_desc_btn{margin:15px 0 0; }
.forgotpwd_btn {margin: 28px 0 0;}

.order-item .fa{color: #9c9c9c;}
.order-item .add .fa{color: red;}
.weui_dialog textarea{border: none;}
.weui_dialog_footer{padding: 10px;border-top: 1px solid #dadada;text-align: right;}
.modal_mask {position: fixed;background: rgba(0,0,0,.6);top: 0;left: 0;width: 100%;height: 100%;z-index: 90;}
.modal_dialog{position: absolute;left: 0;top: 0;z-index: 100;width: 100%;height: 39%;}
.modal_content {border-radius: 3px;background: #fff;margin: 15px;}
.modal_content .modal_text{padding: 15px;}
.modal_content .modal_text textarea{min-height: 80px;border: none;width: 100%;resize: none;font-size: 14px;}
.publish_btn {background:#2791dc;color:#fff;border-radius: 3px;display: inline-block;padding:5px 20px;border: 1px solid #2791dc;margin-left: 10px;}
.cancel_btn{background:#fff;color:#999;border-radius: 3px;display: inline-block;padding:5px 20px;border: 1px solid #dadada;}
.modal_content .modal_text textarea:focus{border: none;outline: none;}
.show{display: block;}
.hide{display: none;}



/*修改信息*/
.edit_pass .weui_cells_form .weui_cell{padding: 0;}
.weui_cell_ft > span{display: inline-block;width: 65px;height: 65px;border-radius: 100px;overflow: hidden;border: 1px solid #dadada;}
.weui_cell_ft > span img{max-width: 100%;}
.userinfo .weui_cell_ft {position: relative;}
.camera {position: absolute;width: 27px;height: 27px;left: -5px;bottom: 4px;text-align: center;border-radius: 100px;background: #2791dc;line-height: 27px;}
.camera i{font-size: 15px;color: #fff;}
.userinfo .weui_cell_hd label{width: 60px;display: inline-block;}
.check i.fa{width: 23px;height: 23px;display: inline-block;line-height: 23px;background:#fff;border: 1px solid #dadada;border-radius: 100px;text-align: center;margin-right: 5px;}
.active.check i.fa{background: #2c92d2;color: #fff;border-color:#2c92d2 }
.check{display: inline-block;margin-right: 10px;}
.userinfo .weui_cell{padding: 15px 5px;}
.userinfo{padding: 0 15px;}
.bind_list .btn_sm .weui_btn{padding: 0;width: 120px;}
.bind_list {margin-top: 10px;}
.bind_list {margin-bottom: 10px;}
.bind_cell .weui_cell_hd label{width: 80px;}
.bind_cell .weui_cell{padding: 15px 0;}
.bind_cell .weui_btn_area {padding: 100px 0 25px;}
.bind_list {margin-top: 10px !important;}
.account_banner {width: 100%;}
.account_banner img{max-width: 100%;display: block;}
.create_account .weui_cell{padding: 15px 6px;}
.create_account .dro_sec{display: block;position: relative;padding: 0;}
.create_account > .weui_cell{display: block;}
.fr{float: right;}
.fl{float: left;}
.create_account .dro_sec > span{width: 88%;display: inline-block;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;color: #999;margin-bottom: 10px;}
.radio{width: 100%;}
.weui_cell_ft i.weui_icon_checked{height: auto;border: none;width: 95%;}
/*.radio{margin-top: 10px;}*/
.radio .weui_cells_radio{margin-top: 0;}
.radio .weui_cells_radio .weui_cell{padding: 12px 7px;border-bottom: 0;border-top: 1px solid #dadada;}
.radio .weui_cells_radio .weui_check+.weui_icon_checked:before{content: '\EA01';font-size: 23px;color: #C9C9C9;}
.radio .weui_cells_radio .weui_check:checked+.weui_icon_checked:before{content: '\EA06';color:#2c92d2; }
.dro_sec b i{font-size: 24px;color: #e6e6e6;}
.create_account .flex{display: flex;}
.create_account .flex .weui_label{width: 70px;color: #999;}
.create_account{padding: 0 15px;}
.create_account .choose_kinds{padding: 0;}
.create_account .choose_kinds span{padding: 20px 0 15px;display: block;font-size: 13px;color: #999;}
.create_account .choose_kinds .weui_cells_checkbox{margin-top: 0;}
.weui_cells_checkbox .weui_check:checked+.weui_icon_checked:before{color: #2c92d2;}
.create_account .choose_kinds .weui_cells_checkbox .weui_cell_hd{padding-right: 0;}
.dro_sec > b{padding-right: 10px;}
.create_account .weui_cells_radio .weui_cell_ft{display: flex;}
.dro_sec .fa-angle-up:before{content: "\f106";}
.weui_cell p {color: #333;font-size: 13px;}
.weui_cell p span{color: #999;}
.account_list .weui_cell{display: block;padding: 15px;}
.account_list {margin: 0;}
.account_list .weui_cell > h2{padding: 8px 0 10px;font-weight: 500;color: #333;font-size: 17px;}
.list_link span{color: #999;padding-right: 10px;}
.list_link {font-size: 13px;}
.list_btn{margin-top: 65px;padding: 15px 15px 0;margin-bottom: 0;}
.modal_bind{width: 100%;height: 100%;position: relative;}
.modal_bind_content{background: #fff;position: fixed;bottom: 0;left: 0;z-index: 102;width: 100%;}
.modal_bind_content .weui_cells{padding: 0 15px;margin-bottom: 55px;}
.weui_cells_btn {margin-top: 20px;position: fixed;bottom: 0;left: 0;width: 100%;}
.wite_bg {background: #e9f4fa;color: #2c92d2;}
.blue_bg {background: #2c92d2;color: #fff;}
.weui_cells_btn a{display: block;text-align: center;font-size: 17px;padding: 8px 0;}
.hide{display: none;}
.show{display: block;}

@media (max-width: 375px) {
    .date_menu .weui-col-33 .black_color{font-size: 17px;line-height: 18px;}
    .date_menu .weui-col-33 p {font-size: 12px;margin-bottom: -2px;}
    .account_item .weui-row{padding: 10px 15px 6px;}
    .account_tab .weui_navbar_item.weui_bar_item_on span{font-size: 13px;}
    .account_tab .weui_navbar_item{ padding: 5px 0;}
    .account_tab .weui_navbar_item.weui_bar_item_on span {padding-bottom: 5px;}
    .weui_cell_title span{font-size: 12px;}
    .account_tab .weui_navbar+.weui_tab_bd {padding-top: 0;}
    .weui_cell_title span{height: 34px;line-height: 34px;white-space: nowrap;}
    .account_tab .weui_cells_access .weui_cell_hd img{max-width: 37px;}
