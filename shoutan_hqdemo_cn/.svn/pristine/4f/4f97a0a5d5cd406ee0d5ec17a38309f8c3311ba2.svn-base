.agree-layer { position: absolute; top:0; left: 0; width: 100%; height: 100%; z-index: 100; background: url("../images/tran.png") repeat;}
.agree-layer .agree-bg {  padding: 15px; width: 100%;  -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box; position: relative; top:20%;}

.agree-layer .weui_cells_form { background: #fff; padding: 15px;border-radius: 4px;}
.agree-layer .weui_cells_form {
    -webkit-animation:bounceIn 1s .2s ease both;
    -moz-animation:bounceIn 1s .2s ease both;}
@-webkit-keyframes bounceIn{
    0%{opacity:0;
        -webkit-transform:scale(.3)}
    50%{opacity:1;
        -webkit-transform:scale(1.05)}
    70%{-webkit-transform:scale(.9)}
    100%{-webkit-transform:scale(1)}
}
@-moz-keyframes bounceIn{
    0%{opacity:0;
        -moz-transform:scale(.3)}
    50%{opacity:1;
        -moz-transform:scale(1.05)}
    70%{-moz-transform:scale(.9)}
    100%{-moz-transform:scale(1)}
}
