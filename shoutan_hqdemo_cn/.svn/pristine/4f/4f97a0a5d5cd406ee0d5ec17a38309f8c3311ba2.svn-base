    .account_tab .weui_cell_primary small{font-size: 12px;}
    .account_tab .weui_cell_ft{font-size: 16px;line-height: 16px;}
    .account_tab .weui_cell_ft .org{font-size: 13px;}
    .account_tab .weui_cells_access a.weui_cell{padding: 12px 0 12px 39px;min-height: inherit;}
    .account_tab .weui_cells_access .weui_cell_hd{margin-top: -18px;}
    .weui_cell_title span b i{width: 6px;height: 6px;margin-top: -2px;}
    .mychart_box .center-align{font-size: 12px;color: #7d7d7d;padding: 7px 0 0;}
    .weui_cell_title span b{color: #979797;}
    .weui_cell, .weui_tab .weui_cell_title{border-bottom: #f5f5f5 solid 1px;}
    .mychart_content .weui-row .weui-col-25{font-size: 13px;}
    .mychart_content .weui-row .weui-col-25.mon{font-size: 12px !important;}
    .mychart_box{margin-bottom: 0;height: 190px;}
    #tab2 .mychart{top: -42px;}
    #tab3 .mychart{top: -34px;}
    .account_head{border-width: 2px;}
    .account_user{padding: 22px 0 10px;}
    .account_head{width: 61px;height: 61px;}
    .account_user h2{font-size: 13px;font-weight: normal;margin: 3px 0 10px;}
    .account_user p small{font-size: 12px;}
    .account_user h1{font-size: 28px;}
    .filter{right: 10px;width: 24px;height: 24px;background: url("../images/filter-icon.png") no-repeat 0 0/ 21px auto;}
    .mychart_content .weui-row{padding: 4px 0;}
    .account_user > p{line-height: 20px;}
    .mychart_content .center-align{padding: 2px 0;font-size: 12px;}
    .btn_items .weui_panel_hd{padding: 15px 0 2px;font-size: 12px;}
    .btn_group .weui_panel_bd a{height: 36px;line-height: 36px;}
    .btn_group .weui_panel_bd{padding: 3px 0 17px;}
    .btn_group .weui_panel_bd a.store{float: left;}
    .mar_bor a{margin: 4px 4px 0 !important;}
    .btn_group .btn_sm .machine{padding-top: 4px;}
    .machine a{margin: 14px 13px 0 0 !important;}
    .btn_sm .btn_group .weui_panel_bd{padding: 10px 15px 18px;}
    .btn_group .btn_items:last-child .machine{padding-top: 0;}
    .btn_group .btn_items{margin: 0 15px;}
    .logo{padding: 70px 94px 46px;}
    .weui_cells_form .weui_cell{padding: 8px 0;}
    .weui_cells_form .weui_input{font-size: 13.5px;}
    .getcode{font-size: 14px;}
    .agree-layer .agree-bg{padding: 40px 23px;}
    .agree-layer .weui_cells_form{padding: 17px 15px 21px;}
    .weui_msg_desc p span{font-size: 14px !important;}
    .agree-bg .weui_msg_desc + .weui_msg_desc_btn{margin-top: 0;padding-top: 16px;}
    .weui_cells_form .weui_msg_desc, #login_form .weui_msg_desc{max-height: 253px;}
    .agree-bg .weui_msg_desc + .weui_msg_desc_btn .weui_btn.agree-btn{font-size: 12px;}
    .weui_cells_form .weui_btn_area {margin-top: 0;padding-top: 14px;margin-bottom: 20px;}
    .code_link a{font-size: 12px;}
    .register_btn{bottom: 59px;}
    .weui_cells_form .weui_btn_area.forgotpwd_btn{margin-top: 42px;}
    .weui_cell p{font-size: 12px;}
    .list_link span, .list_link a{font-size: 12px;}
    .account_list .weui_cell > h2{font-size: 12px;color: #000;padding: 4px 0 4px;}
    .account_list .weui_cell{padding: 10px 15px;}
    .list_btn{margin-top: 234px;}
    .create_account .dro_sec > span{font-size: 12px;}
    .create_account{margin-top: 4px;}
    .dro_sec > b{margin-top: -3px;}
    .create_account .flex .weui_label{font-size: 12px;}
    .create_account .weui_cell.dro_sec{padding: 10px 6px 8px;}
    .create_account .choose_kinds span{font-size: 12px;padding: 20px 0 10px;}
    .weui_cells_checkbox .weui_icon_checked:before{font-size: 19px;}
    .weui_input{font-size: 13px;}


}
@media (max-width: 320px) {
    html,body {  font-size: 13px;}
    .account_head{width: 52px;height: 52px;border-radius: 50%; border: 2px solid #96c9e9;}
    .account_user { padding: 20px 0 10px;}
    .filter { right: 12px;top: 12px; width: 18px; height: 18px; background: url("../images/filter-icon.png") no-repeat 0 0/ 18px auto;}
    .account_user h1 { font-size: 26px;}
    .black_color{ font-size: 16px;}
    .date_menu .weui-col-33 .black_color{font-size: 14px;}
    .account_item .weui-row{padding: 8px 15px 4px;}
    .account_tab .weui_navbar_item{font-size: 12px;}
    .account_tab .weui_navbar_item.weui_bar_item_on span{font-size: 12px;}
    .account_tab .weui_navbar_item{padding: 4px 0;}
    .account_tab .weui_navbar_item.weui_bar_item_on span{padding-bottom: 3px;}
    .weui_cell_title span{height: 31px;line-height: 32px;}
    .account_tab .weui_cells_access .weui_cell_hd img{max-width: 30px;}
    .account_tab .weui_cell_ft{font-size: 15px;}
    .account_tab .weui_cell_primary p{font-size: 12.5px;}
    .account_tab .weui_cells_access a.weui_cell{padding: 9px 0 6px 39px;}
    .account_tab .weui_cells_access .weui_cell_hd{margin-top: -14px;}
    .logo{padding: 60px 81px 41px;}
    .weui_cells_form .weui_input{font-size: 13px;}
    .weui_cells_form .weui_label{height: 15px;background: url("../images/regis-icon.png") no-repeat 0 0 / 15px auto;}
    .weui_cells_form .weui_label.bus-name{background-position: 0 -15px;}
    .weui_cells_form .weui_label.card {background-position: 0 -31px;}
    .weui_cells_form .weui_label.pass-label{background-position: 0 -46px;}
    .getcode{font-size: 12px;padding-left: 15px;height: 22px;line-height: 22px;bottom: -1px;}
    .weui_cells_form .weui_cell{padding: 7px 0;margin: 0 0 8px;}
    .weui_btn_area{padding-top: 13px;}
    .weui_btn{height: 35px;line-height: 35px;font-size: 14px;}
    .agree-layer .agree-bg{padding: 15px 23px;}
    .weui_msg_desc p span strong{font-size: 14px;}
    .weui_msg_desc p{font-size: 12px;}
    .weui_msg_desc p span{font-size: 14px !important; }

}
.account_tab .weui_cells_access a.weui_cell:focus,.account_tab .weui_cells_access a.weui_cell:active{background: #fff !important;}
.btn_group .btn_items,.btn_group .btn_items:focus,.btn_group .btn_items:active{border-bottom: 1px solid #f5f5f5;}
.btn_group .btn_items:last-child{border: none;}
.weui_search_inner{margin: 0;padding: 0;border-bottom: 1px solid #dadada;z-index: 0;}
.btn_group .weui_panel_bd a:first-child{margin-left: 0;}
.weui_search_inner .weui_search_input{border-radius: 0 !important;box-shadow: none !important;}
.weui-picker-container{height: 100%;background: rgba(0,0,0,.5);}
.weui_search_bar{z-index: 0;}

.weui_msg_desc p span strong{color: #2c92d2;font-weight: 500;font-size: 14px;}
.weui_msg_desc p{font-size: 12px;}
.code_link a.blue{color: #2c92d2;}
.resets, .find{position: relative;}
.weui_cells_form .resets .weui_input, .weui_cells_form .find .weui_input{width: 80%;}
.resets .reset_icon,.find .find_icon{position: absolute;right: 6px;top: 0;}
.resets .reset_icon i,.find .find_icon i{color: #e5e5e5;}