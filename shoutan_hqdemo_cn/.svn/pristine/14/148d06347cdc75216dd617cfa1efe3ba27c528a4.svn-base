<include file="Public:header"/>
<!--修改密码-->
<div class="logo">
    <a href=""><img src="__TMPL__Public/images/login_logo.png"/></a>
</div>

<!--表单-->
<div class="weui_cells_form">
        <form class="login content">
            <div class="weui_cell">
                <div class="weui_cell_hd"><label class="weui_label pass-label" for="ypass"><i class="iconfont">&#xe603;</i></label></div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input id="ypass" class="weui_input" type="password" placeholder="原密码">
                </div>
            </div>
            <div class="weui_cell">
                <div class="weui_cell_hd"><label class="weui_label pass-label" for="npass"><i class="iconfont">&#xe603;</i></label></div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input id="npass" class="weui_input" type="password" placeholder="新密码">
                </div>
            </div>
            <div class="weui_cell">
                <div class="weui_cell_hd"><label class="weui_label pass-label" for="qpass"><i class="iconfont">&#xe603;</i></label></div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input id="qpass" class="weui_input" type="password" placeholder="确认密码">
                </div>
            </div>
        </form>

    <p class="weui_btn_area">
        <a href="javascript:;" class="weui_btn weui_btn_warn" title="修改密码">修改密码</a>
    </p>
</div>
<include file="Public:footer"/>